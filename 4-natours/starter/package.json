{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express, and mongodb",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "env NODE_ENV=development nodemon server.js",
    "debug": "nodemon --inspect server.js",
    "delete-data": "env NODE_ENV=LOADER node ./dev-data/data/import-dev-data.js --delete",
    "import-data": "env NODE_ENV=LOADER node ./dev-data/data/import-dev-data.js --import",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled",
    "build": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "stevent",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.0.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "pug": "^3.0.2",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "stripe": "^14.1.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "ndb": "^1.1.5",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.0",
    "prettier": "^3.0.2",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "url": "^0.11.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
